# Configuration SignNow
SIGNNOW_CLIENT_ID=your_client_id_here
SIGNNOW_CLIENT_SECRET=your_client_secret_here
SIGNNOW_TEMPLATE_ID=your_template_id_here

# Configuration serveur en ligne
PORT=3000
NODE_ENV=production
COMPANY_EMAIL=votre-email@votresociete.com

# URLs et domaine
SERVER_URL=https://votre-domaine.com
WEBHOOK_URL=https://votre-domaine.com/api/webhook/signnow

# Sécurité
JWT_SECRET=your_super_secret_key_here
CORS_ORIGIN=https://votre-frontend.com

# Gestion des fichiers temporaires
UPLOAD_MAX_SIZE=10485760
CLEANUP_INTERVAL=3600000

# Logs et monitoring
LOG_LEVEL=info
SENTRY_DSN=your_sentry_dsn_if_needed